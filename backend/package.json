{
  "name": "ddu-backend",
  "version": "0.1.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "gen:test-registry": "ts-node src/gen-test-registry.ts",
    "db:start": "docker run -d -e POSTGRES_DB=service_db -e POSTGRES_USER=postgres -e POSTGRES_PASSWORD=admin -p 5432:5432 --health-cmd pg_isready --health-interval 1s --health-timeout 5s --health-retries 30 postgres:14.5",
    "db:generate-migration": "sequelize-cli migration:generate",
    "build": "tsc",
    "build:docker": "npm install && npm run build && docker build . -t ddu-backend:local",
    "lint": "eslint . --ext .ts",
    "test": "jest ./src",
    "jest": "jest",
    "secrets:replace": "node ./secret-replace.js"
  },
  "author": "The Adventurers of Drunken Dragon Entertainment",
  "license": "MIT",
  "dependencies": {
    "@blockfrost/blockfrost-js": "^5.0.0",
    "@emurgo/cardano-message-signing-nodejs": "^1.0.1",
    "@emurgo/cardano-serialization-lib-nodejs": "^11.2.1",
    "@types/compression": "^1.7.2",
    "@types/random-seed": "^0.3.3",
    "axios": "^0.27.2",
    "bech32": "^2.0.0",
    "bip39": "^3.0.4",
    "cbor": "^8.1.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "express-jwt": "^8.2.1",
    "express-winston": "^4.2.0",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "node-cache": "^5.1.2",
    "pg": "^8.8.0",
    "random-seed": "^0.3.0",
    "sequelize": "^6.21.6",
    "umzug": "^3.2.1",
    "uuidv4": "^6.2.13",
    "winston": "^3.8.2",
    "winston-cloudwatch": "^6.1.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.7.14",
    "nodemon": "^2.0.19",
    "sequelize-cli": "^6.4.1",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  }
}
