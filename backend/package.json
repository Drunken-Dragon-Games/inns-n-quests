{
  "name": "ddu-backend",
  "version": "0.1.0",
  "type": "module",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "dev:mod": "nodemon --exec ts-node -- --experimental-specifier-resolution=node --esm src/server.ts",
    "gen:test-registry": "ts-node src/gen-test-registry.ts",
    "db:start": "docker run -d -e POSTGRES_DB=service_db -e POSTGRES_USER=postgres -e POSTGRES_PASSWORD=admin -p 5432:5432 --health-cmd pg_isready --health-interval 1s --health-timeout 5s --health-retries 30 postgres:14.5",
    "ls": "ls",
    "proxy:start": "docker run --name nginx-proxy -p 80:80 -v $(pwd)/nginx.dev.conf:/etc/nginx/nginx.conf:ro -d nginx:1.13",
    "proxy:start:win": "docker run --name nginx-proxy -p 80:80 -v %cd%/nginx.dev.conf:/etc/nginx/nginx.conf:ro -d nginx:1.13",
    "db:generate-migration": "sequelize-cli migration:generate",
    "build": "tsc",
    "build:docker": "npm install && npm run build && docker build . -t ddu-backend:local",
    "lint": "eslint . --ext .ts",
    "test": "jest ./src",
    "jest": "jest",
    "secrets:replace": "node ./secret-replace.js",
    "babel": "babel src/server.ts --out-file dist/bundle.js"
  },
  "author": "The Adventurers of Drunken Dragon Entertainment",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.342.0",
    "@blockfrost/blockfrost-js": "^5.0.0",
    "@emurgo/cardano-message-signing-nodejs": "^1.0.1",
    "@emurgo/cardano-serialization-lib-nodejs": "^11.3.1",
    "@types/compression": "^1.7.2",
    "@types/random-seed": "^0.3.3",
    "ajv": "^8.12.0",
    "aws-sdk": "^2.1387.0",
    "axios": "^0.27.2",
    "bech32": "^2.0.0",
    "bip39": "^3.0.4",
    "cbor": "^8.1.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "discord.js": "^14.7.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "express-jwt": "^8.2.1",
    "express-winston": "^4.2.0",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "lucid-cardano": "^0.10.1",
    "node-cache": "^5.1.2",
    "pg": "^8.8.0",
    "random-seed": "^0.3.0",
    "sequelize": "^6.21.6",
    "umzug": "^3.2.1",
    "uuidv4": "^6.2.13",
    "winston": "^3.9.0",
    "winston-cloudwatch": "^6.1.1",
    "yaml": "^2.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@types/ajv": "^1.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.15.11",
    "@types/yaml": "^1.9.7",
    "nodemon": "^2.0.19",
    "sequelize-cli": "^6.4.1",
    "shx": "^0.3.4",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
